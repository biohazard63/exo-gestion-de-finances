            "post": {
                "summary": "Store a newly created transaction in storage",
                "operationId": "ab765dced58cfe2f620929db31452145",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "user_id",
                                    "amount",
                                    "type"
                                ],
                                "properties": {
                                    "user_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "amount": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 100.5
                                    },
                                    "type": {
                                        "type": "string",
                                        "enum": [
                                            "credit",
                                            "debit"
                                        ],
                                        "example": "credit"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Payment for services"
